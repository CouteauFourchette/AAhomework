<h1>Sign Up</h1>
<%= render 'errors' %>
<div class='form'>
  <form action="<%= users_url %>" method="post">
    <%= auth_token %>
    <label for="email">Email</label>
    <input id="email" type="text" name="user[email]" value="<%= @user.email %>" placeholder="email">
    <label for="password">Password</label>
    <input id='password' type="password" name="user[password]" value="<%= @user.password %>" placeholder="password">
    <input type="submit" value="Sign Up" class="submit">
  </form>
</div>
